{
    "name": "laravel-notification-channels/authy",
    "description": "Authy notification channel for Laravel, with the ability to send in-app, sms, and call verification tokens.",
    "type": "library",
    "keywords": [
        "sms",
        "call",
        "authy",
        "laravel",
        "verification",
        "rinvex",
        "phone",
        "token"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/laravel-notification-channels/authy/issues",
        "source": "https://github.com/laravel-notification-channels/authy",
        "docs": "https://github.com/laravel-notification-channels/authy/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/laravel-notification-channels/authy/contributors"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/queue": "^8.0.0 || ^9.0.0",
        "rinvex/laravel-authy": "^8.0.0 || ^9.0.0",
        "illuminate/notifications": "^8.0.0 || ^9.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.0",
        "phpunit/phpunit": "^9.5.0",
        "mockery/mockery": "^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Authy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Authy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
